<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:BTQuickie.Converters">

    <converters:BooleanVisibilityConverter x:Key="VisibilityBooleanConverter" />
    <converters:InvertedBooleanVisibilityConverter x:Key="InvertedVisibilityBooleanConverter" />
    <converters:StringVisibilityConverter x:Key="StringToVisibilityConverter" />
    <converters:EmptyStringToParameterConverter x:Key="EmptyStringToParameterConverter" />
    <converters:StringKeyConverter x:Key="StringKeyConverter" />
    <converters:StringModifierKeysConverter x:Key="StringModifierKeysConverter" />
    <converters:PercentDecimalConverter x:Key="PercentDecimalConverter" />
    <converters:StringToLowerConverter x:Key="StringToLowerConverter" />
    <converters:GreaterThanZeroConverter x:Key="GreaterThanZeroConverter" />

</ResourceDictionary>